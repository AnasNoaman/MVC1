<!doctype html>
<html >
<head>
    <meta charset="utf-8">
    <title>Users</title>
</head>
<body>

    <table >
        
        <thead>
            <th>id</th>
            <th>name</th>
            <th></th>
        </thead>
        <tbody>
            @foreach (var item in @Model)
            {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.Name</td>
                    <td><a href="/Users/Details/@item.Id">Details</a></td>
                    <td><a href="/Users/Edit/@item.Id">Edit</a></td>
                    <td><a href="/Users/Delete/@item.Id">Delete</a></td>
                </tr>
            }
           
            
        </tbody>
       
    </table>
    <a href="/Users/Create">Create New User</a>

</body>
</html>
